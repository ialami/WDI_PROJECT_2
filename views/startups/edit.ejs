<h1 class="titlenew">Update start-up profile</h1>
<div class="container mainnewstartup">
  <div class="row">
    <div class="col-lg-12">
<form method="POST" action="/startups/<%= startup.id %>">
  <input type="hidden" name="_method" value="PUT">
  <div class="form-group">

    <label for="name">Name</label>
    <input class="form-control" type="text" name="name" id="name" value="<%= startup.name %>" required>

  </div>
  <div class="form-group">
    <label for="description">Description</label>
    <input class="form-control" type="text" name="description" id="description" value="<%= startup.description %>" required>
  </div>
  <div class="form-group">
    <label for="industry">Industry</label>
    <input class="form-control" type="text" name="industry" id="industry" value="<%= startup.industry %>" required>
  </div>
  <div class="form-group">
    <label for="founders">Founders</label>
    <input class="form-control" type="text" name="founders" id="founders" value="<%= startup.founders %>" required>
  </div>
  <div class="form-group">
    <label for="date">Date</label>
    <input class="form-control" type="number" name="date" id="date" value="<%= startup.date %>" required>
  </div>
  <div class="form-group">
    <label for="country">Country</label>
    <input class="form-control" type="text" name="country" id="country" value="<%= startup.country %>" required>
  </div>
  <div class="form-group">
    <label for="image">Image</label>
    <input class="form-control" type="text" name="image" id="image" value="<%= startup.image %>" required>
  </div>
  <div class="form-group">
    <label for="incubator">Incubator</label>
    <input class="form-control" type="text" name="incubator" id="incubator" value="<%= startup.incubator %>">
  </div>
  <div class="form-group">
    <label for="partnering">Partnering strategy</label>
    <input class="form-control" type="text" name="partnering" id="partnering" value="<%= startup.partnering %>">
  </div>
  <div class="form-group">
    <label for="website">Website URL</label>
    <input class="form-control" type="text" name="website" id="website" value="<%= startup.website %>">
  </div>
  <div class="form-group">
    <label for="fundingtype">Last funding type</label>
    <input class="form-control" type="text" name="fundingtype" id="fundingtype" value="<%= startup.fundingtype %>">

  <button class="btn btn-outline-secondary addbutton">Update start-up</button>
</form>
</div>
</div>
</div>
