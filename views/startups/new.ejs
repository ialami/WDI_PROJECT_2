<h1 class="titlenew">Add a new start-up</h1>
<div class="container mainnewstartup">
  <div class="row">
    <div class="col-lg-12">
<form method="POST" action="/startups">
  <div class="form-group">

    <label for="name">Name</label>
    <input class="form-control" type="text" name="name" id="name" placeholder="Enter name of start-up" required>

  </div>
  <div class="form-group">
    <label for="description">Description</label>
    <input class="form-control" type="text" name="description" id="description" required>
  </div>
  <div class="form-group">
    <label for="industry">Industry</label>
    <input class="form-control" type="text" name="industry" id="industry">
  </div>
  <div class="form-group">
    <label for="founders">Founders</label>
    <input class="form-control" type="text" name="founders" id="founders" required>
  </div>
  <div class="form-group">
    <label for="date">Date</label>
    <input class="form-control" type="number" name="date" id="date" placeholder="Enter year of creation (i.e. 2014)" required>
  </div>
  <div class="form-group">
    <label for="country">Country</label>
    <input class="form-control" type="text" name="country" id="country" required>
  </div>
  <div class="form-group">
    <label for="vision">Vision</label>
    <input class="form-control" type="text" name="vision" id="vision" required>
  </div>
  <div class="form-group">
    <label for="mission">Mission</label>
    <input class="form-control" type="text" name="mission" id="mission" required>
  </div>
  <div class="form-group">
    <label for="image">Image</label>
    <input class="form-control" type="text" name="image" id="image" placeholder="Enter URL" required>
  </div>

  <button class="btn btn-outline-primary addbutton">Add Startup</button>
</form>
</div>
</div>
</div>
