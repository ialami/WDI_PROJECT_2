<h1 class="titlenew">Add a new start-up</h1>
<div class="container mainnewstartup">
  <div class="row">
    <div class="col-lg-12">
<form method="POST" action="/startups">
  <div class="form-group">

    <label for="name"></label>
    <input class="form-control" type="text" name="name" id="name" placeholder="Name of start-up" required>

  </div>
  <div class="form-group">
    <label for="description"></label>
    <input class="form-control" type="text" name="description" id="description" placeholder="Description" required>
  </div>
  <div class="form-group">
    <label for="industry"></label>
    <input class="form-control" type="text" name="industry" id="industry" placeholder="Industry" required>
  </div>
  <div class="form-group">
    <label for="founders"></label>
    <input class="form-control" type="text" name="founders" id="founders" placeholder="Founders" required>
  </div>
  <div class="form-group">
    <label for="date"></label>
    <input class="form-control" type="number" name="date" id="date" placeholder="Founded year (i.e. 2014)" required>
  </div>
  <div class="form-group">
    <label for="country"></label>
    <input class="form-control" type="text" name="country" id="country" placeholder="Country" required>
  </div>
  <div class="form-group">
    <label for="image"></label>
    <input class="form-control" type="text" name="image" id="image" placeholder="Image URL" required>
  </div>
  <div class="form-group">
    <label for="incubator"></label>
    <input class="form-control" type="text" name="incubator" id="incubator" placeholder="Incubator (optional)">
  </div>
  <div class="form-group">
    <label for="partnering"></label>
    <input class="form-control" type="text" name="partnering" id="partnering" placeholder="Partnering strategy">
  </div>
  <div class="form-group">
    <label for="website"></label>
    <input class="form-control" type="text" name="website" id="website" placeholder="Website URL">
  </div>
  <div class="form-group">
    <label for="fundingtype"></label>
    <input class="form-control" type="text" name="fundingtype" id="fundingtype" placeholder="Last funding type (i.e. Seed capital, Angels, Series A, etc.)">
  </div>

  <button class="btn btn-outline-warning addbutton">Add Startup</button>
</form>
</div>
</div>
</div>
